{
    "name":"adblockerr",
    "version":"1.0",
  
    "description": "광고 막아줘요~",
    
      "browser_action": {
        "browser_style": true,
        "default_title": "popup",
         "default_popup" : "popup.html"
      },
    "background":{
        "scripts": [
            "blockedsites.js", "background.js"
        ]
    },
    "permissions":["activeTab","<all_urls>","tabs","webRequest","webRequestBlocking","http://*/", "https://*/"],
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "all_frames": true,
        "js": ["jquery.js",
              "popup.js",
              "upload.js"]
      }
    ],
    "manifest_version":2
}